{
  "urn": "urn:mediator:ICVP-from-bundle",
  "version": "1.0.0",
  "name": "ICVP-from-Bundle Mediator",
  "description": "Ejecuta $icvp por cada Immunization de un Bundle dado (Bundle/{id}/$icvp?immunizationId=...). Incluye endpoint de salud.",
  "defaultChannelConfig": [
    {
      "name": "ICVP From Bundle Channel",
      "urlPattern": "^/icvp/_from-bundle$",
      "routes": [
        {
          "name": "ICVP From Bundle Route",
          "host": "10.68.174.209",
          "port": "8012",
          "path": "/icvp/_from-bundle",
          "primary": true,
          "type": "http",
          "passThroughHeaders": [
            "Accept",
            "Content-Type",
            "Authorization",
            "Origin"
          ]
        }
      ],
      "allow": ["admin"],
      "methods": ["POST"],
      "type": "http"
    },
    {
      "name": "Mediator Health Channel",
      "urlPattern": "^/health$",
      "routes": [
        {
          "name": "Mediator Health Route",
          "host": "10.68.174.209",
          "port": "8012",
          "path": "/health",
          "primary": true,
          "type": "http",
          "passThroughHeaders": []
        }
      ],
      "allow": ["admin"],
      "methods": ["GET"],
      "type": "http"
    }
  ],
  "endpoints": [
    {
      "name": "icvp-from-bundle-endpoint",
      "path": "/icvp/_from-bundle",
      "method": "post"
    },
    {
      "name": "mediator-health-endpoint",
      "path": "/health",
      "method": "get"
    }
  ],
  "heartbeatPath": "/health",
  "heartbeatInterval": 30000
}