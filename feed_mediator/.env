# Dirección del Atom Feed de OpenMRS
ATOM_FEED_URL=https://openmrs.tuservidor.cl/openmrs/ws/atomfeed/encounter/recent

# (Opcional) Usuario y contraseña si el Atom Feed requiere autenticación HTTP Básica
OPENMRS_USER=atomfeeduser
OPENMRS_PASS=atomfeedpassword

# Endpoint del entryfeed de tu main-mediator
OPENHIM_ENTRYFEED=http://main-mediator:4000/entryfeed

# (Opcional) Usuario y contraseña si el endpoint de OpenHIM requiere autenticación
OPENHIM_USER=openhimuser
OPENHIM_PASS=openhimpass

# Intervalo de polling en segundos
POLL_INTERVAL=15
